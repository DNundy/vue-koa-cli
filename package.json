{
  "name": "vue-koa-cli",
  "version": "1.1.0",
  "description": "A whole vue-koa-cli",
  "main": "app.js",
  "scripts": {
    "start"       : "npm run dev_all",

    "dev_fe"      : "set NODE_ENV=development&&node app.js",
    "dev_server"  : "set NODE_ENV=production&&nodemon app.js",
    "dev_all"     : "set NODE_ENV=development&&nodemon app.js",

    "prod_fe"     : "node build/build.js",
    "prod_server" : "set NODE_ENV=production&&node app.js",
    "prod_all"    : "set NODE_ENV=production&&node build/build.js&&node app.js"
  },
